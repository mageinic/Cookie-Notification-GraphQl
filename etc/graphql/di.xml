<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:framework:ObjectManager/etc/config.xsd">
    <type name="Magento\StoreGraphQl\Model\Resolver\Store\StoreConfigDataProvider">
        <arguments>
            <argument name="extendedConfigData" xsi:type="array">
                <item name="cookie_notification_enable_disable"
                      xsi:type="string">mageinic_cookienotification/general/enabledisable</item>
                <item name="box_position"
                      xsi:type="string">mageinic_cookienotification/general/box_position</item>
                <item name="notice_title"
                      xsi:type="string">mageinic_cookienotification/general/notice_title</item>
                <item name="notice_message"
                      xsi:type="string">mageinic_cookienotification/general/notice_message</item>
                <item name="cookie_policy_url"
                      xsi:type="string">mageinic_cookienotification/general/cookie_policy_url</item>
                <item name="behaviour"
                      xsi:type="string">mageinic_cookienotification/general/behaviour</item>
                <item name="expire_cookie_day"
                      xsi:type="string">mageinic_cookienotification/general/expire_cookie_day</item>
            </argument>
        </arguments>
    </type>
</config>
